{
  "name": "tabula-rasa",
  "version": "0.0.1",
  "description": "A profile for Grant Shively.",
  "private": true,
  "main": "dist_node/src/server.js",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "dev-api": "NODE_OPTIONS=\"--loader=ts-node/esm\" TS_NODE_PROJECT=tsconfig.node.json node --inspect --require ts-node/register src/server.ts",
    "build": "prisma migrate dev && astro build && tsc -p tsconfig.node.json",
    "start": "node --max-old-space-size=200 ./dist_node/src/server.js",
    "preview": "astro preview",
    "astro": "astro",
    "subfont": "subfont -ir --no-fallbacks --silent --root dist",
    "generate-blog-post": "node scripts/generate-ai-blog-post.mjs",
    "deploy": "fly deploy",
    "test": "eslint . && astro check && prisma validate",
    "db-studio": "prisma studio",
    "db-migrate": "prisma migrate dev",
    "db-reset": "prisma migrate reset",
    "db-deploy": "prisma migrate deploy",
    "repl": "ts-node -P ./tsconfig.node.json -O '{\"module\": \"commonjs\"}'",
    "update-dependencies": "ncu -x limax -x svgo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gshively11/tabula-rasa.git"
  },
  "author": "gshively11@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gshively11/tabula-rasa/issues"
  },
  "homepage": "https://github.com/gshively11/tabula-rasa#readme",
