    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.19.1",
    "mdast-util-to-string": "^3.1.1",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.3.0",
    "prettier": "^2.8.4",
    "prettier-plugin-astro": "^0.8.0",
